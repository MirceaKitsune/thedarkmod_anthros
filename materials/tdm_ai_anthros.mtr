models/md5/chars/anthros/anthro_hair_yellow
{
	qer_editorimage	models/md5/chars/anthros/anthro_hair_yellow
	surftype15
	description "hair, yellow"

	sort nearest
	nonsolid
	twosided
	noselfshadow
	noshadows

	{
		blend diffusemap
		map models/md5/chars/anthros/anthro_hair_yellow
		alphatest 0.5
	}
	{
		if ( parm11 > 0 )
		blend gl_dst_color, gl_one
		map _white
		rgb 0.40 * parm11
	}
	{
		if ( parm11 > 0 )
		blend add
		map models/md5/chars/anthros/anthro_hair_yellow
		rgb 0.15 * parm11
	}

	bumpmap models/md5/chars/anthros/anthro_hair_local
	specularmap models/md5/chars/anthros/anthro_hair_s
}

